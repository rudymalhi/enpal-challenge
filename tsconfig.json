{
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "module": "NodeNext",
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                    
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
  }
}
